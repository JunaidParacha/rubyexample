      	<option value="DC"<%=session[:DC]%>>District of Columbia</option>
      	<option value="FL"<%=session[:FL]%>>Florida</option>
      	<option value="GA"<%=session[:GA]%>>Georgia</option>
        <option value="HI"<%=session[:HI]%>>Hawaii</option>
        <option value="ID"<%=session[:ID]%>>Idaho</option>
