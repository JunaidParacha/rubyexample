  <fieldset><legend>Update User profile</legend>
    <form class="" action="/logins/<%=session[:user]%>" method="post">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <input type="hidden" name="_method" value="put">
      <input type="text" name="update[first_name]" value="<%=@user.first_name%>">
      <input type="text" name="update[Last_name]" value="<%=@user.Last_name%>">
      <input type="email" name="update[email]" value="<%=@user.email%>">
      <input type="text" name="update[location]" value="<%=@user.location%>">
      <select name="update[state]">
      	<option value="AL"<%=session[:AL]%>>Alabama</option>
